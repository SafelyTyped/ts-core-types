{
  "name": "@safelytyped/core-types",
  "version": "0.0.1",
  "description": "Core safe types for Typescript",
  "repository": "github:safelytyped/ts-core-types",
  "main": "lib/v1/index.js",
  "types": "lib/v1/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc",
    "lint": "tslint -p ./tsconfig.json",
    "prepublishOnly": "npm run test",
    "prepack": "npm run build",
    "test": "npm run build && mocha -r ts-node/register \"src/**/*.spec.ts\"",
    "cover": "rimraf coverage && rimraf .nyc_output && nyc --cache=false npm run test",
    "clean": "rimraf lib"
  },
  "contributors": [
    "Stuart Herbert <stuherbert@ganbarodigital.com> (https://ganbarodigital.com)"
  ],
  "keywords": [
    "architypes",
    "entities",
    "error reporting",
    "nominals",
    "refined types",
    "type refinement",
    "value objects"
  ],
  "files": [
    "lib/**/*",
    "CHANGELOG.md",
    "LICENSE.md"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.1.2",
    "chai": "^4.2.0",
    "chai-bytes": "^0.1.2",
    "chai-string": "^1.5.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.8.0"
  }
}
